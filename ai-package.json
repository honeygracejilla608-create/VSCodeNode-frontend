{
  "name": "ai-gpu-development-stack",
  "version": "1.0.0",
  "description": "Complete AI GPU development environment",
  "main": "ai-server.py",
  "scripts": {
    "ai:dev": "python ai-server.py",
    "ai:jupyter": "jupyter lab --ip=0.0.0.0 --port=8888 --no-browser --allow-root",
    "ai:train": "python train_model.py",
    "ai:inference": "python inference_api.py",
    "ai:gpu-test": "python gpu_test.py",
    "ai:models": "python manage_models.py",
    "docker:ai": "docker build -f Dockerfile.ai -t ai-gpu-dev .",
    "docker:ai:run": "docker run --gpus all -p 8888:8888 -p 3001:3001 -v $(pwd)/ai_models:/app/ai_models -v $(pwd)/ai_data:/app/ai_data ai-gpu-dev",
    "docker:ai:jupyter": "docker run --gpus all -p 8888:8888 -v $(pwd)/notebooks:/app/notebooks ai-gpu-dev jupyter lab --ip=0.0.0.0 --port=8888 --no-browser --allow-root"
  },
  "dependencies": {
    "torch": "^2.1.0",
    "torchvision": "^0.16.0",
    "torchaudio": "^2.1.0",
    "transformers": "^4.35.0",
    "datasets": "^2.14.0",
    "accelerate": "^0.24.0",
    "tensorflow": "^2.14.0",
    "jax": "^0.4.20",
    "flax": "^0.7.4",
    "diffusers": "^0.24.0",
    "opencv-python": "^4.8.0",
    "pillow": "^10.0.0",
    "scikit-image": "^0.21.0",
    "spacy": "^3.7.0",
    "nltk": "^3.8.0",
    "numpy": "^1.25.0",
    "pandas": "^2.1.0",
    "scikit-learn": "^1.3.0",
    "matplotlib": "^3.8.0",
    "seaborn": "^0.13.0",
    "plotly": "^5.17.0",
    "fastapi": "^0.104.0",
    "uvicorn": "^0.24.0",
    "google-cloud-aiplatform": "^1.35.0",
    "wandb": "^0.15.0",
    "gpustat": "^1.1.0",
    "pynvml": "^11.5.0"
  },
  "devDependencies": {
    "pytest": "^7.4.0",
    "pytest-cov": "^4.1.0",
    "black": "^23.0.0",
    "isort": "^5.12.0",
    "flake8": "^6.1.0",
    "mypy": "^1.7.0",
    "pre-commit": "^3.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "gpu",
    "machine-learning",
    "deep-learning",
    "computer-vision",
    "nlp",
    "pytorch",
    "tensorflow",
    "cuda"
  ],
  "author": "AI Developer",
  "license": "MIT"
}
