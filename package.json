{
  "name": "vscode-node",
  "version": "1.0.0",
  "description": "Todo API with JWT",
  "main": "server/index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build",
    "start": "NODE_ENV=production tsx server/index.ts",
    "deploy-ai": "node deploy-ai.js",
    "demo": "node demo-api-keys.js",
    "demo:help": "node demo-api-keys.js --help",
    "docker:build": "docker build -t todo-api .",
    "docker:run": "docker run -p 3001:3001 --env-file .env todo-api",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "test": "echo \"Tests would run here\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@vitejs/plugin-react": "^4.0.0",
    "dotenv": "^16.0.0",
    "drizzle-orm": "^0.28.0",
    "drizzle-zod": "^0.5.0",
    "express": "^4.18.0",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "firebase-frameworks": "^0.4.2",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^5.0.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.0",
    "pg": "^8.11.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "resend": "^6.1.2",
    "vite": "^5.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^8.10.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
